<%title "Results"%>
<h3><%=@title%></h3><br/>
<%if @profiles %>
<%if @profiles.length > 0 %>
	<%= render :partial=>'partials/profiles/profile_attributes', :locals => {:profiles => @profiles}%>
    <%= will_paginate @profiles %>
    <%=link_to "Export To Excel",{:controller=>"search",:action=>"export_qualification_results",:category=>params[:category],:location=>params[:location]},:class=>'button-link'%>
<%else%>
    <h3><span style="color:red">No Records Found</span></h3>
<%end%>
<%end%>